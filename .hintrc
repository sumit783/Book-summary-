{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-color",
          "backdrop-filter"
        ]
      }
    ],
    "typescript-config/consistent-casing": "off",
    "typescript-config/strict": "off",
    "no-inline-styles": "off",
    "typescript-config/is-valid": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}